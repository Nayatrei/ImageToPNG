{
  "manifest_version": 3,
  "name": "Image to SVG Converter",
  "version": "1.2.0",
  "description": "Adds a context menu item on images to convert them to SVG with advanced options.",
  "permissions": [
    "contextMenus",
    "downloads",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}